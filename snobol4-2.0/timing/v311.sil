       ACOMPC  YPTR,0,,PRINT5,UNTERR
PRINT6 AEQLC   ZPTR,0,PRINT2	   Check for defaulted format
       LOCAPT  TPTR,OTSATL,YPTR,PRINT4
*				   Insert length
PRINT3 LOCAPV  ZPTR,OUTATL,XPTR,PRINT1
