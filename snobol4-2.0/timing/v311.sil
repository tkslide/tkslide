       RCALL   XPTR,INTVAL,,FAIL   Get offset			[PLB52]
       PUSH    XPTR		   Save offset			[PLB52]
       RCALL   YPTR,INTVAL,,FAIL   Get whence			[PLB52]
       POP     (XPTR,XCL)	   Restore offset, unit		[PLB52]
